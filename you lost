  #include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    int lowerLimit = 1;   
    int upperLimit = 10;   
    int randomNumber;
 int num1, num2;
    srand(time(NULL));

      printf("enter number   ");
      scanf("%d"  ,&num1);
    randomNumber = (rand() % (upperLimit - lowerLimit + 1)) + lowerLimit;
num2=num1+randomNumber ;
printf("mine is %d \n"  ,num2);
printf("you lost   :)  ");
    return 0;
}
